<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hss.modules.system.mapper.BaseDictTypeMapper">

    <update id="updateNameAndEnName">
        update BASE_DICT_TYPE set NAME = #{name},EN_NAME = #{enName} where id = #{id}
    </update>
    <delete id="deleteDictById">
        delete from BASE_DICT_TYPE where id = #{id}
    </delete>
</mapper>